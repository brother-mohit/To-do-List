
<%-  include('header') -%>

 <div class="box" id="heading">
  <h1> <%=dayname%> </h1>
 </div>


 <div class="box">
   <% for(let i=0; i<newitems.length; i++) { %>

    <div class="item">

        <input type="checkbox" >
         <p><%=newitems[i]%></p>
    </div>


   <% } %>



 <form class="item" action="/" method="post">

   <input type="text" name="item" placeholder="New Item">
   <button type="submit" name="button">+</button>

 </form>

 </div>

<%-  include('footer') -%>
